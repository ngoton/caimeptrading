        var time = dt.getHours() + ":" + dt.getMinutes();
        var day = dt.getDate() + "-" + ( (dt.getMonth()+1)<10?("0"+(dt.getMonth()+1)):(dt.getMonth()+1) ) + "-" + dt.getFullYear();

        var min = $(this).parent().attr('min');
        var max = $(this).parent().attr('max');

        var today = $(this).attr('max');

        var data = $(this).attr('data');
        var index = $(this).index();

        var value = false;
        $('.table_data').find('tr').each(function(){
          var td = $(this).children('td').eq(index);
          if (td.text() != "") {
            value = true;
          }
      });
        

        if ($(this).html() == "" && (min <= dt.getHours() && dt.getHours() < max) && day == today && value == false) {
            $(this).html('<button>x</button>');

            $('button').click(function(){
                  $.ajax({
                        url: '<?php echo BASE_URL ?>/checkin/add',
                        type: 'POST',
                        data: {time:time, data:data},
                        success:function(success){
                            //alert(success);
                        }
                    });
                  $(this).parent().html(time);
            });
        }
    });
    $('td').mouseleave(function(){
        if ($(this).html() == "<button>x</button>") {
            $(this).html('');
        }
    });


    $('.table_data > tbody  > tr').each(function() {
      var dt = new Date();
        var min = $(this).attr('min');
        var max = $(this).attr('max');
        if (min <= dt.getHours() && dt.getHours() < max) {
            $(this).css({'background':'rgba(0, 208, 255, 0.09)'});
        }
    });

    setInterval(function(){
         $('.table_data > tbody  > tr').each(function() {
            var dt = new Date();
              var min = $(this).attr('min');
              var max = $(this).attr('max');
              if (min <= dt.getHours() && dt.getHours() < max) {
                  $(this).css({'background':'rgba(0, 208, 255, 0.09)'});
